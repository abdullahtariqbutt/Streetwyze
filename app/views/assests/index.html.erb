<h1>Assests</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Category</th>
      <th>Leave rating</th>
      <th>Rating</th>
      <th>Stuff type</th>
      <th>Story</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @assests.each do |assest| %>
      <tr>
        <td><%= assest.name %></td>
        <td><%= assest.address %></td>
        <td><%= assest.category %></td>
        <td><%= assest.leave_rating %></td>
        <td><%= assest.rating %></td>
        <td><%= assest.stuff_type %></td>
        <td><%= assest.story %></td>
        <td><%= link_to 'Show', assest %></td>
        <td><%= link_to 'Edit', edit_assest_path(assest) %></td>
        <td><%= link_to 'Destroy', assest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Assest', new_assest_path %>
