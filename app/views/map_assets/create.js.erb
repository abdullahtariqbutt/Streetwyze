<% if @map_asset.errors.any? %>
  $("#new_asset_form").html("<%= escape_javascript render('form', map_asset: @map_asset) %>");
  $("#new_asset_form").rating_binder();
<% else %>
  $("#assets_table").prepend("<%= escape_javascript render('map_asset_row', asset: @map_asset) %>");
  $(".close-modal").click()
<% end %>


